
/**
 * Write a description of class HeightCrazy here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
public class HeightCrazy
{
    private int Height;
    
    public HeightCrazy(int tall)
    {
        Height = tall;
    }
    
    public void setHeight(int tall)
    {
        Height = tall;
    }
    
    public int getHeight()
    {
        return Height; 
    }
}    
  
  
/**
 * Write a description of class Book here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
public class Book
{
    private String title;
    private String author;
    
    public Book(String t, String a)
    {
        title = t;
        author = a;
    }
    
    public void setTitle(String t)
    {
        title = t;
    }
    
    public void setAuthor(String a)
    {
        author = a;
    }
}

/**
 * Write a description of class Book here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
public class BookV2
{
    private String title;
    private String author;
    
    public BookV2(String t, String a)
    {
        this.title = t;
        this.author = a;
    }
    
    public void setTitle(String t)
    {
        this.title = t;
    }
    
    public void setAuthor(String a)
    {
        this.author = a;
    }
}



/**
 * Write a description of class TreasureChest here.
 *
 * @author (your name)
 * @version (a version number or a date)
 */
public class TreasureChest
{
    private double treasure;
    private String treasureAsString;
    
    public TreasureChest(double t)
    {
        treasure = 5.0;
        treasure = t;
        String treasureAsString = new Double(treasure).toString();
    }
    
    public void setTreasure(double t)
    {
        treasure = t;
    }
    
    public double getTreasure()
    {
        return treasure;
    }
    
    public String toString()
    {
        return treasureAsString;
    }
}





  





